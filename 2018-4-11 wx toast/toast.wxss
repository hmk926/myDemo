.toast_content_box{
  display:flex;
  width:100%;
  height:100%;
  justify-content:center;
  align-items:center;
  position:fixed;
  z-index:1000;
}

.toast_content{
  width: 50%;
  height:200rpx;
  padding:30rpx;
  background:rgba(0, 0, 0, 0.8);
  border-radius: 20rpx;
  line-height:200rpx;
}

.toast_content_text{
  height: 100%;
  width: 100%;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
}
